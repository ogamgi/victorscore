<query id="deleteLang" action="delete">
    <tables>
        <table name="lang" />
    </tables>
    <conditions>
        <condition operation="equal" column="site_srl" var="site_srl" default="0" />
        <condition operation="equal" column="name" var="name" notnull="notnull" pipe="and" />
    </conditions>
</query>
